  "workflows": {
    "multimodal_content_creation": {
      "name": "Multimodal Content Creation Pipeline",
      "description": "Audio transcription -> text analysis -> image generation pipeline",
      "nodes": [
        {
          "id": "audio_transcription",
          "model": "whisper_large",
          "inputs": ["audio_file"],
          "outputs": ["transcribed_text"]
        },
        {
          "id": "content_analysis",
          "model": "llama2_13b_base",
          "inputs": ["transcribed_text"],
          "outputs": ["content_summary", "image_concepts"]
        },
        {
          "id": "image_prompt_generation",
          "model": "code_llama_7b",
          "inputs": ["image_concepts"],
          "outputs": ["detailed_image_prompt"]
        },
        {
          "id": "high_quality_image",
          "model": "stable_diffusion_xl",
          "inputs": ["detailed_image_prompt"],
          "outputs": ["hq_image"],
          "config_options": ["width", "height", "steps", "guidance_scale"]
        },
        {
          "id": "fallback_image",
          "model": "stable_diffusion_v1_5",
          "inputs": ["detailed_image_prompt"],
          "outputs": ["fallback_image"],
          "config_options": ["width", "height", "steps"]
        }
      ],
      "edges": [
        {"from": "audio_transcription", "to": "content_analysis"},
        {"from": "content_analysis", "to": "image_prompt_generation"},
        {"from": "image_prompt_generation", "to": "high_quality_image"},
        {"from": "image_prompt_generation", "to": "fallback_image"}
      ]
    },
    "advanced_code_analysis": {
      "name": "Advanced Code Analysis and Documentation",
      "description": "Multi-stage code analysis with specialized models",
      "nodes": [
        {
          "id": "initial_code_scan",
          "model": "code_llama_7b",
          "inputs": ["source_code"],
          "outputs": ["code_structure", "complexity_metrics"]
        },
        {
          "id": "detailed_analysis",
          "model": "code_llama_13b",
          "inputs": ["code_structure"],
          "outputs": ["detailed_analysis"]
        },
        {
          "id": "security_review",
          "model": "mixtral_8x7b",
          "inputs": ["source_code", "detailed_analysis"],
          "outputs": ["security_report"]
        },
        {
          "id": "performance_analysis",
          "model": "mixtral_8x7b",
          "inputs": ["complexity_metrics", "detailed_analysis"],
          "outputs": ["performance_report"]
        },
        {
          "id": "comprehensive_review",
          "model": "llama2_70b_base",
          "inputs": ["security_report", "performance_report", "detailed_analysis"],
          "outputs": ["final_documentation"]
        }
      ],
      "edges": [
        {"from": "initial_code_scan", "to": "detailed_analysis"},
        {"from": "initial_code_scan", "to": "performance_analysis"},
        {"from": "detailed_analysis", "to": "security_review"},
        {"from": "detailed_analysis", "to": "performance_analysis"},
        {"from": "security_review", "to": "comprehensive_review"},
        {"from": "performance_report", "to": "comprehensive_review"},
        {"from": "detailed_analysis", "to": "comprehensive_review"}
      ]
    },
    "research_synthesis_pipeline": {
      "name": "Research Synthesis with Multiple Expert Models",
      "description": "Complex research workflow with multiple parallel analysis paths",
      "nodes": [
        {
          "id": "research_query_analysis",
          "model": "llama2_13b_base",
          "inputs": ["research_query"],
          "outputs": ["analysis_plan", "key_concepts"]
        },
        {
          "id": "technical_analysis",
          "model": "code_llama_13b",
          "inputs": ["analysis_plan"],
          "outputs": ["technical_insights"]
        },
        {
          "id": "domain_analysis_1",
          "model": "mixtral_8x7b",
          "inputs": ["key_concepts"],
          "outputs": ["domain_insights_1"]
        },
        {
          "id": "domain_analysis_2",
          "model": "mixtral_8x7b",
          "inputs": ["key_concepts"],
          "outputs": ["domain_insights_2"]
        },
        {
          "id": "semantic_embedding",
          "model": "clip_vit_large",
          "inputs": ["key_concepts"],
          "outputs": ["concept_embeddings"]
        },
        {
          "id": "synthesis_preparation",
          "model": "llama2_13b_base",
          "inputs": ["technical_insights", "domain_insights_1", "domain_insights_2"],
          "outputs": ["prepared_synthesis"]
        },
        {
          "id": "final_synthesis",
          "model": "llama2_70b_base",
          "inputs": ["prepared_synthesis", "concept_embeddings"],
          "outputs": ["comprehensive_research_report"]
        }
      ],
      "edges": [
        {"from": "research_query_analysis", "to": "technical_analysis"},
        {"from": "research_query_analysis", "to": "domain_analysis_1"},
        {"from": "research_query_analysis", "to": "domain_analysis_2"},
        {"from": "research_query_analysis", "to": "semantic_embedding"},
        {"from": "technical_analysis", "to": "synthesis_preparation"},
        {"from": "domain_analysis_1", "to": "synthesis_preparation"},
        {"from": "domain_analysis_2", "to": "synthesis_preparation"},
        {"from": "synthesis_preparation", "to": "final_synthesis"},
        {"from": "semantic_embedding", "to": "final_synthesis"}
      ]
    }
  }
} 